pipeline {
     agent any
	tools{
		maven 'maven'
	}

    stages {
        stage('Build') {
            steps {
            echo 'hello chayafghjk'
                bat script: 'mvn clean package'
            }
	    post{
	     	success {
		echo 'chaya'
		archiveArtifacts '**/*.war'
		}
	    }
        }

        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
